/* common css end */
.get-started-form {
  max-width: 700px;
  width: auto;
  margin: 0 auto;
  padding: 20px;
  background-color: #ffffff;
}
.equity-form{
  max-width: 700px;
  width:auto;
  margin: 0 auto;
  padding: 20px;
  background-color: #ffffff;
}
.form-group {
  position: relative;
  /* margin-bottom: 20px; */
}

.form-group .nf-control {
  width: 100%;
  border: none;
  border-bottom: 1px solid #ccc;
  padding: 11px 15px;
  background: none;
  font-size: 14px;
  box-sizing: border-box;
  transition: border-color 0.3s, background-color 0.3s;
  font-weight: 700;
  /* color: #868e96; */
  color: #606266;
  border-radius: 0;
}

.form-group .nf-control:focus {
  border-bottom: 2px solid #868e96;
  outline: none;
  color: #606266;
}

.form-group .nf-label {
  position: absolute;
  top: 50%;
  left: 15px;
  transform: translateY(-50%);
  font-size: 14px;
  pointer-events: none;
  transition: all 0.2s ease;
    border-color: #000000;
    color: #868e96;
    font-weight: 700;
}
.form-group .nf-label.hidden-label {
  opacity: 0;
  visibility: hidden;
}
.nf-btn {
  padding: 14px;
  line-height:1.5;
  border: none;
  border-radius: 5px;
  font-size: 14px;
  cursor: pointer;
  /* box-shadow: 0 20px 38px rgba(0, 0, 0, 0.16); */
  background-color: rgba(242, 117, 33, 1);
  border-color: rgba(242, 117, 33, 1);
  color: #ffffff;
  min-width: 200px;
  transition: .2s linear;
  user-select: none;
  vertical-align: middle;
  white-space: nowrap;
  font-weight: 600;

}
.nf-btn-priamry{
  background-color: rgba(242, 117, 33, 1);
    border-color: rgba(242, 117, 33, 1);
    color: #ffffff;
}
.nf-btn[disabled]{
  opacity: 0.5;
  cursor: not-allowed;
  transform: translateY(0px);
  pointer-events: none;
}
.nf-btn.nf-btn-priamry:hover {
  background-color: #114a8a;
  transform: translateY(-3px);
    -webkit-transform: translateY(-3px);
}

/* dropdown css */
.form-group {
  position: relative;
  margin-bottom: 15px;
}

.form-group .nf-label {
  position: absolute;
  top: 10px;
  left: 10px;
  font-size: 16px;
  color: #555;
  transition: opacity 0.2s, visibility 0.2s, top 0.2s, font-size 0.2s;
}

.form-group .nf-label.hidden-label {
  opacity: 0;
  visibility: hidden;
}

.form-group .nf-form-select {
  width: 100%;
  padding: 11px 15px;
  border: none;
  border-bottom: 1px solid #ccc;
  background: none;
  font-size: 14px;
  font-weight: 600;
  color: #606266;
  appearance: none;
}

.form-group .nf--form-select:focus {
  outline: none;
  border-bottom: 2px solid #606266;
}

.form-group .nf-form-select:focus ~ label,
.form-group .nf-form-select:not([value=""]) ~ label {
  top: -15px;
  font-size: 12px;
  color: #606266;
}
.form-group.nf-select::after {
  content: 'â–¼'; /* Arrow character */
  position: absolute;
  right: 10px;
  top: 50%;
  transform: translateY(-50%);
  pointer-events: none;
  font-size: 12px;
  color: #606266;
}

.form-group .nf-select:focus ~ ::after {
  color: #606266;
}
