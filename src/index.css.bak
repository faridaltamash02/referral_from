  pointer-events: none;
  transition: all 0.2s ease;
    border-color: #000000;
    color: #868e96;
    font-weight: 700;
}
.form-group .nf-label.hidden-label {
  opacity: 0;
  visibility: hidden;
}
.nf-btn {
  cursor: pointer !important;
  min-width: 125px;
  width: 100%;
  font-family: glence-heavy;
  padding: 14px !important;
  border: 1px solid transparent;
  line-height: 1.5;
  user-select: none;
  vertical-align: middle;
  white-space: nowrap;
  font-size: 0.6em !important;
  font-weight: 600 !important;
  box-sizing: border-box;
  transition: opacity .45s cubic-bezier(0.25,1,0.33,1), transform .45s cubic-bezier(0.25,1,0.33,1), border-color .45s cubic-bezier(0.25,1,0.33,1), color .45s cubic-bezier(0.25,1,0.33,1), background-color .45s cubic-bezier(0.25,1,0.33,1), box-shadow .45s cubic-bezier(0.25,1,0.33,1);
}
.nf-btn-priamry{
  background-color: rgba(242, 117, 33, 1);
  border-color: rgba(242, 117, 33, 1);
  color: #ffffff;
}
.nf-btn[disabled] {
  opacity: 0.5;
  cursor: not-allowed !important;
  transform: translateY(0px);
}

.nf-btn.nf-btn-primary:hover:not([disabled]) {
  background-color: #114a8a !important;
  transform: translateY(-3px);
  -webkit-transform: translateY(-3px);
  box-shadow: 0 10px 20x rgba(0,0,0,0.15) !important;
}
.nf-btn-priamry.nf-single{
  width: auto;
}
/* dropdown css */
.form-group {
  position: relative;
  margin-bottom: 15px;
}

.form-group .nf-label {
  position: absolute;
  top: 10px;
  left: 10px;
  font-size: 16px;
  color: #555;
  transition: opacity 0.2s, visibility 0.2s, top 0.2s, font-size 0.2s;
}

.form-group .nf-label.hidden-label {
  opacity: 0;
  visibility: hidden;
}

.form-group .nf-form-select {
  width: 100%;
  border: none;
  background: none;
  font-size: 14px;
  font-weight: 700;
  color: #606266;
  appearance: none;
  border-radius: 0 !important;
  padding: 13px 15px;
  background-clip: padding-box;
  font-family: -apple-system, "system-ui", Segoe UI, Roboto, Oxygen-Sans, Ubuntu, Cantarell, Helvetica Neue, sans-serif;
  line-height: 1;
  transition: border-color .15s ease-in-out, box-shadow .15s ease-in-out;
  border-bottom: 1px solid #ced4da !important;
}
.form-group .nf-select option {
  font-size: 16px; /* Adjust as needed */
  font-weight: 700 !important;
  }
.form-group .nf--form-select:focus {
  outline: none;
  border-bottom: 2px solid #606266;
}

.form-group .nf-form-select:focus ~ label,
.form-group .nf-form-select:not([value=""]) ~ label {
  top: -15px;
  font-size: 12px;
  color: #606266;
}

.dropdown-container {
  position: relative;
  display: inline-block;
}

.dropdown-container::after {
  content: "";
  position: absolute;
  right: 10px;
  top: 50%;
  transform: translateY(-50%);
  pointer-events: none;
  width: 0;
  height: 0;
  border-left: 6px solid transparent;
  border-right: 6px solid transparent;
  border-top: 6px solid #606266;
}

.form-group .nf-select:focus ~ ::after {
  color: #606266;
}

.nf-select ~ .select2.select2-container{display: none !important;} 
